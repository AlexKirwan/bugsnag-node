{
    "name": "bugsnag",
    "description": "Bugsnag notifier for node.js scripts",
    "version": "1.9.1",
    "main": "./lib/bugsnag.js",
    "homepage": "http://bugsnag.com",
    "dependencies": {
        "promise": "7.x",
        "stack-trace": "0.0.9",
        "request": "2.x"
    },
    "devDependencies": {
        "mocha": "latest",
        "mocha-lcov-reporter": "~1.3.0",
        "jscover": "~1.0.0",
        "chai": "~1.5.0",
        "sinon": "latest",
        "grunt": "~0.4.0",
        "grunt-bumpx": "latest",
        "express": "~3.4.2"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:bugsnag/bugsnag-node.git"
    },
    "keywords": [
        "error",
        "bugsnag",
        "exception"
    ],
    "license": "MIT",
    "scripts": {
        "test": "mocha",
        "coverage": "jscover lib lib-cov && BUGSNAG_COV=1 mocha -R html-cov > coverage.html"
    },
    "engine": {
        "node": ">=0.8"
    }
}
